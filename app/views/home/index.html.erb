<h1 class="text-center">FoodGood</h1>

<% if current_user %>
    <%= current_user.name %>
    <%= current_user.email %>
    <% if current_user.avatar? %>
        <div class="container ">
            <%= image_tag current_user.avatar, class: "img-fluid rounded-circle" %>
        </div> 

    <% end %>
<% end %>

<div id="edit-user-form"></div>
